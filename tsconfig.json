{
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "target": "ES2022",
    "skipLibCheck": true,
    "strictNullChecks": true,
    "paths": {
      "@ui/*": ["./app/ui/*"],
      "@api/*": ["./app/api/*"],
      "@hooks/*": ["./app/hooks/*"],
      "@appTypes/*": ["./app/types/*"],
      "@db/*": ["./db/*"],
      "@components/*": ["./app/components/*"],
      "@modules/*": ["./app/modules/*"]
    }
  }
}
